{
    "mqtt":{
        "brokerIP" : "192.168.32.50",
        "brokerPort" : 1883,
        "brokerProto" : "tcp",
        "subscribedTopics":["bot/jetson/detectSingleObject",
                            "bot/jetson/detectObjectDisplayVideo",
                            "bot/killSwitch",
                            "bot/logger"],
        "publishingTopics":["client/moveBaseFeedback",
                            "client/moveEndEffectorFeedback",
                            "client/ErrorHandling"]
