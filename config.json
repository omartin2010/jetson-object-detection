        "detection_threshold": 0.75,
        "open_cv_default_tracker" : "kcf",
        "k4a_device" :{
            "color_resolution" : "K4A_COLOR_RESOLUTION_720P",
            "depth_mode": "K4A_DEPTH_MODE_WFOV_UNBINNED",
